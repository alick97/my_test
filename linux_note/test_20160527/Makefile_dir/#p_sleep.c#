#include<stdio.h>

int main()
{
	// 没有\n
	// 则 先输出的输出缓冲区 等程序执行结束才 输出到显示器上
	// 所以 执行结果是 先睡2秒 而后输出
    //printf("%-20s","123456");
	
	// 有\n \n是换行 
    printf("%20s\n","123456");
    sleep(2);
	return 0;
}
